{
  "name": "gatsby_strapi_gitpod",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install-all": "npm-run-all -l -p install:*",
    "develop:frontend": "cd frontend && npm run develop",
    "develop:backend": "cd backend && npm run develop",
    "develop-all": "npm-run-all -l -p develop:*",
    "start:frontend": "wait-on http://localhost:1337/admin && cd frontend && npm run start",
    "start:backend": "cd backend && npm run start",
    "start-all": "npm-run-all -l -p start:*"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "wait-on": "5.2.1"
  }
}
