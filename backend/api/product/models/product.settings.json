{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "images": {
      "collection": "file",
      "via": "related",
      "allowedTypes": ["images", "files", "videos"],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "product-extra.variant"
    },
    "categories": {
      "via": "products",
      "collection": "category"
    },
    "dimensions": {
      "type": "enumeration",
      "enum": ["two", "three"]
    }
  }
}
